{
  "name": "tester-bundler",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "compile:client": "tsc -w -p src/client/tsconfig.json",
    "compile:server": "tsc -w -p src/server/tsconfig.json",
    "server:dev": "SET DEBUG=tester-bundler* & nodemon ./dist/bin/www",
    "server": "node ./dist/bin/www",
    "prebuild:ru": "tslint --test src",
    "build:ru": "rollup -c rollup.config.main.js",
    "watch:main": "tslint --test src & rollup -c rollup.config.main.js -w",
    "watch:services": "tslint --test src & rollup -c rollup.config.services.js -w",
    "pretest": "npm run build",
    "test": "mocha"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "ejs": "~2.5.2",
    "express": "~4.14.0",
    "hashids": "^1.1.1",
    "httpmodule": "^1.0.0",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.9.3",
    "morgan": "~1.7.0",
    "parallax": "^1.0.0",
    "serve-favicon": "~2.3.0",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.0",
    "@types/cookie-parser": "^1.3.30",
    "@types/greensock": "^1.15.32",
    "@types/mongoose": "^4.7.9",
    "@types/morgan": "^1.7.32",
    "@types/serve-favicon": "^2.2.28",
    "@types/uuid": "^2.0.29",
    "tslib": "^1.5.0",
    "tslint": "^4.4.2",
    "typescript": "^2.2.2"
  }
}
